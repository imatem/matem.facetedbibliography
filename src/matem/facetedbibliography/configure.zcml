<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

   <include package="Products.CMFCore" file="permissions.zcml" />

  <browser:page
      name="faceted_bib"
      for="*"
      class=".faceted.FacetedView"
      template="faceted_view.pt"
      permission="zope.Public"
      />
  <browser:resource
    name="cssfaceted.css"
    file="./css/cssfaceted.css"
    />
  <browser:resource
    name="bootstrap.css"
    file="./css/bootstrap.css"
    />
  <browser:resource
    name="bootstrap.js"
    file="./js/bootstrap.js"
    />
  <browser:resource
    name="jquery.collapsible.js"
    file="./js/jquery.collapsible.js"
    />
  <browser:resource
    name="jquery.collapse_cookie_storage.js"
    file="./js/jquery.collapse_cookie_storage.js"
    />
  <browser:resource
    name="jquery.collapse_storage.js"
    file="./js/jquery.collapse_storage.js"
    />
 </configure>
